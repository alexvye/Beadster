package vye.rest.controller.fixture;

import vye.core.events.*;
import static vye.rest.controller.fixture.RestDataFixture.*;

import java.util.Date;
import java.util.UUID;

public class RestEventFixture {
  public static CardDrawnEvent drawCard(UUID key) {
    return CardDrawnEvent.drawCard(key);
  }
  public static CardPlayedEvent playCard(UUID key, String status) {
    return new CardPlayedEvent(key, new OrderStatusDetails(new Date(), status));

  }
}