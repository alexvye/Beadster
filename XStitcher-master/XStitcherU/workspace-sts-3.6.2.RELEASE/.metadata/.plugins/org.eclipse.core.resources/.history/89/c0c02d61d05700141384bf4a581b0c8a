package vye.domain;

enum CARD_TYPE{
	  	Beast, Demon, Other
	};

public class Card {
	private int manaCost;
	private int attack;
	private int maxHealth;
	private int currentHealth;
	private DeathRattle deathRattle;
	private BattleCry battleCry;
	private boolean hasDivineShield;
	private String name;
	private String imageUrl;
	private String descriptiveText;
	private String voiceUrl;
	private CARD_TYPE type;
}
